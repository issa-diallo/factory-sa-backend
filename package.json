{
  "name": "factory-backend",
  "version": "1.0.0",
  "description": "",
  "main": "dist/config/bootstrap.entry.js",
  "scripts": {
    "dev": "NODE_ENV=development tsx watch src/config/bootstrap.entry.ts",
    "dev:debug": "NODE_ENV=development node --inspect-brk=0.0.0.0:9229 ./node_modules/tsx/dist/cli.mjs src/config/bootstrap.entry.ts",
    "start": "NODE_ENV=production node dist/config/bootstrap.entry.js",
    "build": "rm -rf dist && prisma generate && tsc && pnpm run copy-swagger-files && pnpm run prisma:generate:dist",
    "copy-swagger-files": "copyfiles -u 1 \"src/swagger/openapi.yaml\" \"src/swagger/swagger.html\" dist",
    "copy-swagger-ui": "copyfiles -u 1 \"node_modules/swagger-ui-dist/**/*\" dist/swagger-ui-dist",
    "prisma:generate": "prisma generate",
    "prisma:generate:dist": "copyfiles -u 1 \"src/generated/**/*\" dist",
    "prisma:studio:kill": "docker exec factory-backend-backend-1 pkill -f \"prisma studio\" || true",
    "prisma:studio:start": "docker exec factory-backend-backend-1 pnpm prisma studio --port 5555 --hostname 0.0.0.0",
    "prisma:studio:reset": "pnpm prisma:studio:kill && pnpm prisma:studio:start",
    "db:migrate": "docker exec -it factory-backend-backend-1 pnpm prisma migrate deploy",
    "db:seed": "prisma db seed",
    "db:reset": "pnpm db:migrate && pnpm db:seed",
    "build:watch": "tsc --watch",
    "test:coverage": "NODE_ENV=test jest --coverage",
    "test": "NODE_ENV=test JWT_SECRET_DEVELOPPEMENT=test_secret NODE_OPTIONS='--experimental-vm-modules' jest --config jest.config.mjs --runInBand",
    "lint": "eslint . --ext .ts,.js --fix",
    "lint:check": "eslint . --ext .ts,.js",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "outdated": "pnpm outdated"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.12.4",
  "dependencies": {
    "@prisma/client": "6.16.2",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "5.1.0",
    "express-rate-limit": "^8.1.0",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "reflect-metadata": "^0.2.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "tsyringe": "^4.10.0",
    "xlsx": "^0.18.5",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/preset-env": "^7.28.3",
    "@eslint/js": "^9.37.0",
    "@faker-js/faker": "^10.0.0",
    "@types/bcrypt": "^6.0.0",
    "@types/cors": "^2.8.19",
    "@types/express": "5.0.3",
    "@types/jest": "^30.0.0",
    "@types/js-yaml": "^4.0.9",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.7.1",
    "@types/supertest": "^6.0.3",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@typescript-eslint/eslint-plugin": "^8.46.0",
    "@typescript-eslint/parser": "^8.46.0",
    "@vercel/node": "^5.3.26",
    "babel-jest": "^30.2.0",
    "copyfiles": "^2.4.1",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^30.2.0",
    "jest-express": "^1.12.0",
    "openapi-types": "^12.1.3",
    "prettier": "^3.6.2",
    "prisma": "^6.17.1",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "2.0.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.0"
  }
}
