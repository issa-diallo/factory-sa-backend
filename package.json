{
  "name": "factory-backend",
  "version": "1.0.0",
  "description": "",
  "main": "dist/src/config/server.bootstrap.js",
  "type": "module",
  "scripts": {
    "dev": "NODE_ENV=development tsx watch src/config/server.bootstrap.ts",
    "dev:debug": "NODE_ENV=development node --inspect-brk=0.0.0.0:9229 ./node_modules/tsx/dist/cli.mjs src/config/server.bootstrap.ts",
    "start": "NODE_ENV=production node dist/src/config/server.bootstrap.js",
    "build": "rm -rf dist && prisma generate && tsc",
    "prisma:generate": "prisma generate",
    "prisma:studio:docker": "docker exec factory-backend-backend-1 pnpm prisma studio --port 5555 --hostname 0.0.0.0",
    "db:seed": "prisma db seed",
    "build:watch": "tsc --watch",
    "test:coverage": "NODE_ENV=test jest --coverage",
    "test": "NODE_ENV=test JWT_SECRET_DEVELOPPEMENT=test_secret NODE_OPTIONS='--experimental-vm-modules' jest --runInBand",
    "lint": "eslint . --ext .ts,.js --fix",
    "lint:check": "eslint . --ext .ts,.js",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "outdated": "pnpm outdated"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.12.4",
  "dependencies": {
    "@prisma/client": "6.11.1",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "5.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.1",
    "reflect-metadata": "^0.2.2",
    "tsyringe": "^4.10.0",
    "xlsx": "^0.18.5",
    "zod": "^4.0.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.31.0",
    "@faker-js/faker": "^9.9.0",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.19",
    "@types/express": "5.0.3",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.0.13",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.36.0",
    "@typescript-eslint/parser": "^8.36.0",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.1",
    "jest": "^30.0.4",
    "jest-express": "^1.12.0",
    "prettier": "^3.6.2",
    "prisma": "^6.11.1",
    "supertest": "^7.1.3",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "2.0.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.36.0"
  }
}
