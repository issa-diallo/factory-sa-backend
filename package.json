{
  "name": "factory-backend",
  "version": "1.0.0",
  "description": "",
  "main": "dist/config/server.bootstrap.js",
  "scripts": {
    "dev": "NODE_ENV=development ts-node-dev --respawn --transpile-only --prefer-ts-exts --require tsconfig-paths/register src/config/server.bootstrap.ts",
    "start": "NODE_ENV=production node dist/config/server.bootstrap.js",
    "build": "prisma generate && tsc",
    "prisma:build": "ts-node prisma/build-schema.ts",
    "prisma:generate": "pnpm run prisma:build && prisma generate",
    "db:seed": "prisma db seed",
    "build:watch": "tsc --watch",
    "test": "NODE_ENV=test jest",
    "test:watch": "NODE_ENV=test jest --watch",
    "test:coverage": "NODE_ENV=test jest --coverage",
    "lint": "eslint . --ext .ts,.js --fix",
    "lint:check": "eslint . --ext .ts,.js",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.12.4",
  "_moduleAliases": {
    "@": "dist",
    "@controllers": "dist/controllers",
    "@routes": "dist/routes",
    "@utils": "dist/utils",
    "@interfaces": "dist/types",
    "@middleware": "dist/middleware",
    "@schemas": "dist/schemas",
    "@services": "dist/services",
    "@constants": "dist/constants"
  },
  "dependencies": {
    "@prisma/client": "6.11.1",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "express": "4.21.2",
    "jsonwebtoken": "^9.0.2",
    "module-alias": "^2.2.3",
    "multer": "^2.0.1",
    "xlsx": "^0.18.5",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.19",
    "@types/express": "4.17.21",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/module-alias": "^2.0.4",
    "@types/multer": "^1.4.13",
    "@types/node": "^24.0.7",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.35.0",
    "@typescript-eslint/parser": "^8.35.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.1",
    "jest": "^30.0.3",
    "prettier": "^3.6.2",
    "prisma": "^6.11.1",
    "supertest": "^7.1.1",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "1.1.8",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3"
  }
}
